---
interface Props {
    size: number;
    className?: string;
}

const { size, className } = Astro.props;
---

<svg
    class:list={[className]}
    width={size}
    height={size}
    viewBox="0 0 24 24"
    fill="none"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"
    stroke-linejoin="round"
    aria-hidden="true"
    ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
        d="M5 13a7 7 0 1 0 14 0a7 7 0 0 0 -14 0z"></path><path
        d="M14.5 10.5l-2.5 2.5"></path><path d="M17 8l1 -1"></path><path
        d="M14 3h-4"></path></svg
>
