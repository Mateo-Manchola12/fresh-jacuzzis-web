---
interface Props {
  size: number
  className?: string
}

const { size, className } = Astro.props
---

<svg
  class:list={[className]}
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  aria-hidden="true"
  ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path d="M4 6l16 0"></path><path
    d="M4 12l16 0"></path><path d="M4 18l16 0"></path></svg
>
